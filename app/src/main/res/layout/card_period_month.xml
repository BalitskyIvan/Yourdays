            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fourth_week_wed"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fourth_week_thu"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fourth_week_fri"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fourth_week_sat"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fourth_week_sun"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/week_fifth_markup"
        style="@style/Text12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/five"
        app:layout_constraintBottom_toBottomOf="@id/card_period_month_fifth_week"
        app:layout_constraintEnd_toStartOf="@id/card_period_month_fifth_week"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/card_period_month_fifth_week" />

    <LinearLayout
        android:id="@+id/card_period_month_fifth_week"
        android:layout_width="291dp"
        android:layout_height="33dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="6dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/card_period_month_fourth_week">

        <FrameLayout
            android:id="@+id/fifth_week_mon"
            android:layout_width="33dp"
            android:layout_height="33dp" />

        <FrameLayout
            android:id="@+id/fifth_week_tue"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fifth_week_wed"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fifth_week_thu"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fifth_week_fri"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fifth_week_sat"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />

        <FrameLayout
            android:id="@+id/fifth_week_sun"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginStart="10dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/card_period_days_markup"
        android:layout_width="291dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="6dp"
        android:orientation="horizontal"
        android:paddingBottom="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/card_period_month_fifth_week">

        <TextView
            android:id="@+id/markup_day_mon"
            style="@style/Text10sp.Center"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:text="@string/mon" />

        <TextView
            android:id="@+id/markup_day_tue"
            style="@style/Text10sp.Center"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="@string/tue" />

        <TextView
            android:id="@+id/markup_day_wed"
            style="@style/Text10sp.Center"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="@string/wed" />

        <TextView
            android:id="@+id/markup_day_thu"
            style="@style/Text10sp.Center"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="@string/thu" />

        <TextView
            android:id="@+id/markup_day_fri"
            style="@style/Text10sp.Center"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="@string/fri" />

        <TextView
            android:id="@+id/markup_day_sat"
            style="@style/Text10sp.Center"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="@string/sat" />

        <TextView
            android:id="@+id/markup_day_sun"
            style="@style/Text10sp.Center"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="@string/sun" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>