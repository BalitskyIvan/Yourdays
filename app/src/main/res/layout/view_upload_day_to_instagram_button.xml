<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <ImageView
        android:id="@+id/button_upload_to_stories_icon"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_marginStart="13dp"
        android:src="@drawable/inst"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <TextView
        android:id="@+id/button_upload_to_stories_text"
        style="@style/Text12sp.Thin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:text="upload to stories"
        app:layout_constraintBottom_toBottomOf="@id/button_upload_to_stories_icon"
        app:layout_constraintStart_toEndOf="@id/button_upload_to_stories_icon"
        app:layout_constraintTop_toTopOf="@id/button_upload_to_stories_icon" />

</merge>